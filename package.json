{
  "name": "@moesif/react-markdown-loader",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Webpack GitHub-flavored Markdown (GFM) loader",
  "keywords": [
    "webpack",
    "loader",
    "markdown",
    "react"
  ],
  "homepage": "https://github.com/moesif/react-markdown-loader",
  "bugs": {
    "url": "https://github.com/moesif/react-markdown-loader/issues"
  },
  "author": {
    "name": "Hugo Manrique",
    "url": "https://hugmanrique.me",
    "email": "npm@hugmanrique.me"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "main": "dist/cjs.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/moesif/react-markdown-loader"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production babel src -d dist --ignore 'src/**/*.test.js'",
    "clean": "del-cli dist",
    "prepublish": "npm run build",
    "release": "standard-version",
    "start": "npm run build -- -w",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "babel-core": "^7.0.0-bridge.0",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "jest": "^29.6.1",
    "standard-version": "^9.5.0",
    "strip-indent": "^4.0.0"
  },
  "dependencies": {
    "@mapbox/jsxtreme-markdown": "^3.0.2",
    "loader-utils": "^3.2.1"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "text",
      "html"
    ],
    "testMatch": [
      "**/test/*.js"
    ]
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "peerDependencies": {
    "webpack": "^3.0.0 || ^4.0.0"
  }
}
